<% if @event.end_date.to_date > Date.today%>
	<div id = "attendee">
	<% if @event.attendees.where(user_id: current_user.id).present? %>
		<%= link_to "UnAttend", unattend_event_path(@event),remote: true,method: 'post',class: "btn btn-primary" %>
	<% else %>
		<%= link_to "Attend", attend_event_path(@event),remote: true,method: 'post',class: "btn btn-primary" %>
	<% end %>
	</div>
<% end %>


