<div id="user_list">
	<table class="table">
	  <thead>
	    <tr>
	      <th scope="col"></th>
	      <th scope="col">Name</th>
	      <th scope="col">Gender</th>
	    </tr>
	  </thead>
	  <tbody>
	  	<% if attendee_users.present? %>
		  	<% attendee_users.each_with_index do |attendee,i| %>
			    <tr>
			      <th scope="row"><%= i+1 %></th>
			      <td><%= attendee.user.name %></td>
			      <td><%= attendee.user.gender %></td>
			    </tr>
				<% end %>
			<% end %>
	  </tbody>
	</table>
</div>